unit Employees.Model;

interface

type
  TEmployeesModel = class
  private
    FID: Integer;
    procedure SetID(const Value: Integer);
  public
    property ID: Integer read FID write SetID;
  end;

implementation

var
  SQLCmd := 'select ID, FIRSTNAME, MIDDLENAME, LASTNAME, BIRTHDAY, DEPARMENT_ID, IS_ACTIVE, UPDTM, REGTM, USER_ID from EMPLOYEES';

{ TEmployeesModel }

procedure TEmployeesModel.SetID(const Value: Integer);
begin
  FID := Value;
end;

end.
